{
    "name": "weather animation",
    "description": "make your screen display weather",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        
      }
    },
    "icons": {
      "16": "/icons/weather-16.png",
      "32": "/icons/weather-32.png",
      "64": "/icons/weather-64.png",
      "128": "/icons/weather-128.png"
    },
    "options_page": "options.html"
}
  